{
    "compilerOptions": {
      "target": "es5",
      "module": "es6",
      "lib": ["es6", "dom"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "jsx": "react-jsx"
    },
    "include": ["src"]
}
  